<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:TheSSS.DicomViewer.Presentation.Views.Tabs"
                    xmlns:vm="clr-namespace:TheSSS.DicomViewer.Presentation.ViewModels.Tabs">
    
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    
    <DataTemplate DataType="{x:Type vm:IncomingPrintQueueTabViewModel}">
        <views:IncomingPrintQueueTabView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:LocalStorageTabViewModel}">
        <views:LocalStorageTabView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:QueryRetrieveTabViewModel}">
        <views:QueryRetrieveTabView/>
    </DataTemplate>
</ResourceDictionary>